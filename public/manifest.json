{
    "manifest_version": 3,
    "name": "SnuggleMeet",
    "version": "1.0.0",
    "description": "A simple React app as a Chrome extension",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "cookies",
      "tabs"
    ]
  }